
                        <h1 class="page-header">Categorias</h1>
                            <% if notice %>
                                <div class="alert alert-success alert-dismissable">
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <%= notice%>
                                </div>
                            <%end%>
                                    <div class="panel panel-default">
                                        <div class="panel-reading">
                                            Listando Categorias
                                            <%= link_to new_backoffice_category_path do %>
                                                <button type="button" class="btn btn-success">Nova Categoria</button>
                                            <%end%>
                                        </div>
                                        <!-- /.panel-heading -->
                                        <div class="panel-body">
                                            <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>Descrição</th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% @categories.each do |category| %>
                                                        <tr>
                                                            <td><%=category.id%></td>
                                                            <td><%=category.description%></td>
                                                            <td style = "width: 150px">
                                                                <%= link_to edit_backoffice_category_path(category.id), class: "btn btn-primary btn-circle" do %>
                                                                    <i class="fa fa-pencil"></i>
                                                                <%end%>
                                                                <button type="button" class="btn btn-danger btn-circle"><i 
                                                                    class="fa fa-times"></i></button>
                                                            </td>
                                                        </tr>
                                                    <% end %>
                                                </tbody>
                                            </table>
                                            </div>
                                        </div>
                                        <!-- /. panel-body -->
                                    </div>                       	                       

    <!-- jQuery -->
    <script src="../vendor/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="../vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="../vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="../dist/js/sb-admin-2.js"></script>
